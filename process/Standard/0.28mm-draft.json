{
  "op3d_schema": "process",
  "op3d_schema_version": "0.1.0",
  "id": "Standard/0.28mm-draft",
  "maintainer": {
    "name": "OpenPrint3D Community",
    "maintainer_type": "community",
    "website": "https://github.com/OpenPrint3D/OpenPrint3D"
  },
  "name": "0.28mm Draft",
  "intent": "draft",
  "layer_height": { "min": 0.24, "max": 0.32, "default": 0.28 },
  "wall_settings": { "wall_count": 2, "top_layers": 4, "bottom_layers": 3 },
  "infill": {
    "density_default": 15,
    "density_range": { "min": 5, "max": 40 },
    "recommended_patterns": ["grid", "lines"]
