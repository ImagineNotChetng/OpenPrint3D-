  "speed": { "outer_wall": 80, "inner_wall": 120, "infill": 150, "top_bottom": 70, "travel": 200 },
  "accel": { "default": 5000, "outer_wall": 3000, "infill": 8000 },
  "retraction": { "distance": 0.8, "speed": 40 },
  "cooling": { "fan_default": 80, "fan_min_layer_time": 8 },
  "supports": { "enabled_default": false, "overhang_threshold": 60 },
  "adhesion": { "default_type": "skirt", "brim_width": 5 },
  "quality_bias": { "priority": "speed" },
  "tags": ["draft", "fast", "prototyping", "0.28mm"],
  "notes": "Fast draft profile for prototyping and test fits. Visible layer lines but significantly faster print times. Good for iteration."
